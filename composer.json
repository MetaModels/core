{
  "name": "metamodels/core",
  "description": "MetaModels core",
  "keywords": [
    "contao",
    "metamodels",
    "core"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/core/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/core"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao-community-alliance/composer-plugin": "^2.4",
    "contao-community-alliance/dc-general": "^2.0.0",
    "contao-community-alliance/dependency-container": "^1.8",
    "contao-community-alliance/event-dispatcher": "~1.3",
    "contao-community-alliance/events-contao-bindings": "^3.5",
    "contao-community-alliance/meta-palettes": "^1.11",
    "contao-community-alliance/url-builder": "^1.3",
    "contao/core": "^3.5.5",
    "discordier/justtextwidgets": "^1.1",
    "doctrine/cache": "^1.6",
    "menatwork/contao-multicolumnwizard": "^3.3.16"
  },
  "require-dev": {
    "metamodels/base-unit-tests": "^1.0.4",
    "metamodels/phpunit-contao-database": "^1.0.1",
    "phpcq/all-tasks": "^1.2"
  },
  "autoload": {
    "psr-0": {
      "MetaModels\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "MetaModels\\Test\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodels"
      },
      "runonce": [
        "runonce/runonce.php"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
