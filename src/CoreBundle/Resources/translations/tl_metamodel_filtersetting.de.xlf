        <target>Einstellung bearbeiten</target>
      </trans-unit>
      <trans-unit id="edit.description" resname="edit.description">
        <source>Edit the filter setting ID %id%</source>
        <target>Die Filtereinstellung ID %id% bearbeiten</target>
      </trans-unit>
      <trans-unit id="copy.label" resname="copy.label">
