        <target>Filtereinstellung kopieren</target>
      </trans-unit>
      <trans-unit id="copy.description" resname="copy.description">
        <source>Copy the filter setting ID %id%</source>
        <target>Kopieren Sie die Filtereinstellung ID %id%</target>
      </trans-unit>
      <trans-unit id="cut.label" resname="cut.label">
        <source>Cut filter setting definition</source>
        <target>Filtereinstellungen ausschneiden</target>
      </trans-unit>
      <trans-unit id="cut.description" resname="cut.description">
        <source>Cut the filter setting ID %id%</source>
        <target>Filtereinstellungen ID %id% ausschneiden.</target>
      </trans-unit>
      <trans-unit id="delete.label" resname="delete.label">
        <source>Delete filter setting</source>
        <target>Filtereinstellung löschen</target>
      </trans-unit>
      <trans-unit id="delete.description" resname="delete.description">
        <source>Delete the filter setting ID %id%</source>
        <target>Die Filtereinstellung Id %id% löschen</target>
      </trans-unit>
      <trans-unit id="delete.confirm" resname="delete.confirm">
        <source>Do you really want to delete element ID %id%?</source>
        <target>Soll das Element ID %id% wirklich gelöscht werden?</target>
      </trans-unit>
      <trans-unit id="show.label" resname="show.label">
        <source>Filter setting details</source>
        <target>Details der Filtereinstellungen</target>
      </trans-unit>
      <trans-unit id="show.description" resname="show.description">
        <source>Show details of filter setting ID %id%</source>
        <target>Die Details der Filtereinstellung ID %id% anzeigen</target>
      </trans-unit>
      <trans-unit id="toggle.label" resname="toggle.label">
        <source>Toggle</source>
        <target>Wechseln</target>
      </trans-unit>
      <trans-unit id="toggle.description" resname="toggle.description">
        <source>Toggle the state of setting ID %id%</source>
        <target>Wechseln des Status der Einstellung ID %id%</target>
      </trans-unit>
      <trans-unit id="typenames.idlist" resname="typenames.idlist">
        <source>Predefined set of items</source>
        <target>Vordefinierter Satz von Items</target>
      </trans-unit>
      <trans-unit id="typenames.simplelookup" resname="typenames.simplelookup">
        <source>Simple lookup</source>
        <target>Einfache Abfrage</target>
      </trans-unit>
      <trans-unit id="typenames.customsql" resname="typenames.customsql">
        <source>Custom SQL</source>
        <target>Eigenes SQL</target>
      </trans-unit>
