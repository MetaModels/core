      </trans-unit>
      <trans-unit id="pastenew.label" resname="pastenew.label">
        <source>Create a new element at the top</source>
      </trans-unit>
      <trans-unit id="pastenew.description" resname="pastenew.description">
        <source>Create a new element after element ID %id%</source>
      </trans-unit>
      <trans-unit id="pasteafter.label" resname="pasteafter.label">
        <source>Paste at the top</source>
      </trans-unit>
      <trans-unit id="pasteafter.description" resname="pasteafter.description">
        <source>Paste after element ID %id%</source>
      </trans-unit>
      <trans-unit id="pasteinto.label" resname="pasteinto.label">
        <source>Create new setting at the top</source>
      </trans-unit>
      <trans-unit id="pasteinto.description" resname="pasteinto.description">
