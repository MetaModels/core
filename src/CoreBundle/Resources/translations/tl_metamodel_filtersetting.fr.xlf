        <target>Détails du paramétrage du filtre</target>
      </trans-unit>
      <trans-unit id="show.description" resname="show.description">
        <source>Show details of filter setting ID %id%</source>
        <target>Afficher les détails du paramétrage du filtre ID %id%</target>
      </trans-unit>
      <trans-unit id="toggle.label" resname="toggle.label">
        <source>Toggle</source>
      </trans-unit>
      <trans-unit id="toggle.description" resname="toggle.description">
        <source>Toggle the state of setting ID %id%</source>
      </trans-unit>
      <trans-unit id="typenames.idlist" resname="typenames.idlist">
